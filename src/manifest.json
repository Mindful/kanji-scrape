{
  "manifest_version": 2,

  "name": "Kanji-Scrape",
  "description": "This extension scrapes your current page and alerts you of any Kanji you haven't seen before.",
  "version": "0.1",

  "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["jquery-1.10.2.min.js", "scrape.js"]
      }
    ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}